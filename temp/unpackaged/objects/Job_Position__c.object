    <validationRules>
        <fullName>Job_Position_is_US_Location</fullName>
        <active>true</active>
        <description>For Job Position is US Location, HR Manager Mailing Address should be US or United States or USA</description>
        <errorConditionFormula>AND(
ISPICKVAL(Location__c, &apos;USA&apos;),
NOT(OR(
HR_Manager__r.MailingCountry = &apos;US&apos;,
HR_Manager__r.MailingCountry = &apos;USA&apos;,
HR_Manager__r.MailingCountry = &apos;United States&apos;
))
)</errorConditionFormula>
        <errorDisplayField>HR_Manager__c</errorDisplayField>
        <errorMessage>HR Manager Mailing Address should be US or United States or USA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Open_Due_Date_can_not_be_in_the_past</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Open_Due_Date__c  &lt;  TODAY(),
ISNEW())</errorConditionFormula>
        <errorDisplayField>Open_Due_Date__c</errorDisplayField>
        <errorMessage>Open Due Date  can not be  in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Record_Type_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordType.Name = &quot;External Position&quot;,
 ISBLANK(Client__c)
)</errorConditionFormula>
        <errorMessage>For External positions is requred to specify the Client</errorMessage>
    </validationRules>
